<Window x:Class="WpfAppDriver.AllReportsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppDriver"
        mc:Ignorable="d"
        Title="AllReportsWindow" Height="369.328" Width="370.588">
    <Grid>
        <StackPanel>
            <Calendar x:Name="calendar" SelectedDatesChanged="Calendar_SelectedDatesChanged"/>
            <Label HorizontalContentAlignment="Center" Content="KM: " FontSize="20" FontWeight="Medium"/>
            <TextBox IsEnabled="False" x:Name="KM" HorizontalContentAlignment="Center" Margin="55,0" FontSize="20" FontWeight="Medium"/>
            <Label HorizontalContentAlignment="Center" Content="Money: " FontSize="20" FontWeight="Medium"/>
            <TextBox IsEnabled="False" x:Name="Money" HorizontalContentAlignment="Center" Margin="55,0" FontSize="20" FontWeight="Medium"/>
        </StackPanel>
    </Grid>
</Window>
